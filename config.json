{
  "projectConfig": {
    "name": "shop-client-api",
    "version": "1.0.0",
    "description": "test description",
    "author": "Jorge Interno",
    "entryFile": "server.js"
  },
  "environment": {
    "target": {
      "envName": "APP_API_URL",
      "value": "https://www.mysale.ph"
    },
    "baseResourcePath": {
      "envName": "BASE_RESOURCE_PATH",
      "value": "/"
    },
    "nonProxyPath": {
      "envName": "NON_PROXY_PATH",
      "value": "/mobile"
    },
    "port": {
      "envName": "PORT",
